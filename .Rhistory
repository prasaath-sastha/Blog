# Define the custom boxplot function
boxplot_custom <- function(data, x, xlab = "X-Axis", ylab = "Y-Axis", main = "My Box Plot", col = "blue") {
# Manually find the outliers based on the IQR rule
data_outliers <- data %>%
mutate(Q1 = quantile({{x}}, 0.25),
Q3 = quantile({{x}}, 0.75),
IQR = Q3 - Q1,
Lower = Q1 - 1.5 * IQR,
Upper = Q3 + 1.5 * IQR) %>%
filter({{x}} < Lower | {{x}} > Upper)
# Generate the plot
ggplot(data, aes(x = factor(1), y = {{x}})) +
geom_boxplot(outlier.colour = "red", color = col) +  # Boxplot
geom_label_repel(data = data_outliers, aes(label = country, y = {{x}}), nudge_x = 0.2) +  # Outlier labels
labs(x = xlab, y = ylab, title = main) +  # Labels
theme_minimal()  # Minimal theme
}
